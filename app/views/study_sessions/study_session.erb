<h2>Welcome, <%= current_student.name %></h2>
<h3>All Study Sessions:</h3>

<% Study_session.all.each do |study_session| %>
  <h4><%= study_session.date %></h4>
  <p><%= study_sessions.hours %><p>
  <p><%= study_session.summary %></p>
<% end %>

<br>

<% if logged_in? %>
  <a href="/study_sessions/new_study_session">New Study Session</a><br>
  <a href="/logout">Log Out</a><br>
<% end %>
