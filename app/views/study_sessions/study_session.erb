<h2>Welcome, <%= current_student.name %>!</h2>
<h4>Here are your study sessions:</h3>

<br>

<% Study_session.all.each do |study_session| %>
  <div>
    <hr>
    <p>Date: <%= study_session.date %></p>
    <p>Hours Studied: <%= study_session.hours %><p>
    <p>Learned: <%= study_session.summary %></p>
    <a href="/study_sessions/<% study_session.id %>">Edit</a>
    <hr>
  <div>
    <br>
<% end %>

<br>

<% if logged_in? %>
  <a href="/study_sessions/new_study_session">New Session</a><br>
  <a href="/logout">Log Out</a><br>
<% end %>
