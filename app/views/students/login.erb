<% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %>

<h3>Log In</h3>
<form action='/login' method="POST">
  Username: <input type="text" name="username" id="username"><br>
  Password: <input type="password" name="password" id="password"><br>
  <br>
<input type="submit" id="submit" value="Log In">
</form>
